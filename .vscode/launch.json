{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Core Launch (web)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "launchSettingsFilePath": "${workspaceFolder}/src/Borealis.Web/Properties/launchSettings.json",
        "program": "${workspaceFolder}/src/Borealis.Web/bin/Debug/net9.0/Borealis.Web.dll",
        "args": [],
        "cwd": "${workspaceFolder}/src/Borealis.Web/",
        "stopAtEntry": false,
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Views"
        }
      },
      {
        "name": "Attach to .NET Functions",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command:azureFunctions.pickProcess}"
      }
    ]
}
